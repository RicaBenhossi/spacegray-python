	"tokenColors": [
	//Python
		{
			"scope": "invalid.illegal.line.continuation.python",
			"settings": {
				"foreground": "#920000",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Source Python",
			"scope": [
				"source.python"
			],
			"settings": {
				"foreground": "#bebdb9"
			}
		},
		{
			"name": "Comment Line Python",
			"scope": [
				"comment.line.number-sign.python"
			],
			"settings": {
				"foreground": "#909090"
			}
		},
		{
			"name": "String Quoted Python",
			"scope": [
				"string.quoted.single.python",
				"string.interpolated.python",
				"meta.fstring.python"
			],
			"settings": {
				"foreground": "#badaba"
			}
		},
		{
			"name": "Support Type Python",
			"scope": [
				"support.type.python"
			],
			"settings": {
				"foreground": "#af89d3",
				"fontStyle": "italic"
			}
		},
		{
			"name": "DocStrings Pyhton",
			"scope": [
				"string.quoted.docstring.multi.python"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#87aa9b"
			}
		},
		{
			"name": "F String Python",
			"scope": [
				"meta.fstring.python",
			],
			"settings": {
				"foreground": "#cc99cc"
			}
		},
		{
			"name": "Function Call Python",
			"scope": [
				"meta.function-call.generic.python"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#a3b8df"
			}
		},
		{
			"name": "Integers Python",
			"scope": "constant.numeric.dec.python",
			"settings": {
				"foreground": "#e4a381"
			}
		},
		{
			"name": "Floats Python",
			"scope": "constant.numeric.float.python",
			"settings": {
				"foreground": "#f99157"
			}
		},
		{
			"name": "Boolean Python",
			"scope": [
				"none",
				"constant.language.python"
			],
			"settings": {
				"foreground": "#e47676"
			}
		},
		{
			"name": "Punctuation Python",
			"scope": [
				"punctuation.definition.string",
				"punctuation.definition.variable",
				"punctuation.definition.string",
				"punctuation.definition.parameters",
				"punctuation.definition.string",
				"punctuation.definition.array",
				"punctuation.section.function.begin.python",
				"punctuation.section.class.begin.python",
				"punctuation.separator.arguments.python",
				"punctuation.separator.colon.python"

			],
			"settings": {
				"foreground": "#d3d0c8"
			}
		},
		{
			"name": "Control Flow Python",
			"scope": [
				"keyword.control.flow.python",
				"keyword.control.import.python",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#cc99ccff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Functions Name Python",
			"scope": [
				"meta.function.python",
				"entity.name.function.python",
				"punctuation.separator.parameters.python"
			],
			"settings": {

				"foreground": "#6d90d1"
			}
		},
		{
			"name": "Classes Python",
			"scope": [
				"meta.class.python",
				"entity.name.type.class.python",
				"punctuation.separator.inheritance.python"
			],
			"settings": {
				"foreground": "#ffcc66"
			}
		},
		{
			"name": "Math Comparison Operators Python",
			"scope": [
				"keyword.operator.arithmetic.python",
				"keyword.operator.comparison.python"
			],
			"settings": {
				"foreground": "#e4a908"
			}
		},
		{
			"name": "Support",
			"scope": [
				"support.function",
				"support.function.builtin.python"
			],
			"settings": {
				"foreground": "#66cccc",
				"fontStyle": "italic"

			}
		},
	//ORIGINAL
