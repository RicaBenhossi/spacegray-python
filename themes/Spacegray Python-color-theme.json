			"settings": {
				"foreground": "#99cc99"
			}
		},
		{
			"name": "Integers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#f99157"
			}
		},
		{
			"name": "Floats",
			"scope": [
				"none"
			],
			"settings": {
				"foreground": "#f99157"
			}
		},
		{
			"name": "Boolean",
			"scope": [
				"none"
			],
			"settings": {
				"foreground": "#f99157"
			}
		},
		{
			"name": "Constants",
